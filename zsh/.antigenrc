# Load the oh-my-zsh's library
antigen use oh-my-zsh

antigen bundles <<EOBUNDLES

# Bundles from the default repo (robbyrussell's oh-my-zsh)
git
urltools
osx
docker
vi-mode
autojump
history
colored-man-pages

# Syntax highlighting bundle.
zsh-users/zsh-syntax-highlighting

# This must come after zsh-syntax-highlighting
zsh-users/zsh-history-substring-search

# Fish-like auto suggestions
zsh-users/zsh-autosuggestions

# Extra zsh completions - Not using b/c it may slow stuff down
#zsh-users/zsh-completions

supercrabtree/k

EOBUNDLES

# Load custom zsh scripts from oh-my-zsh
# antigen bundle $HOME/dotfiles/oh-my-zsh/custom
# TODO Move these to the dotfile repo
antigen bundle $HOME/.dotfiles/zsh-files --no-local-clone

# Load the theme
#antigen theme fribmendes/geometry

#ZSH_THEME="robbyrussell"
#ZSH_THEME="flazz"
#ZSH_THEME="skaro"
#ZSH_THEME="spaceship"
#ZSH_THEME="agnoster"
# Fav - ZSH_THEME="junkfood"
# nice one - miloshadzic
#ZSH_THEME="sporty_256"

# Spaceship Theme
# https://github.com/denysdovhan/spaceship-zsh-theme
antigen theme https://github.com/denysdovhan/spaceship-zsh-theme spaceship

# Tell antigen that you're done
antigen apply
